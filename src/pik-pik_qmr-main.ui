<interface>
  <!-- interface-name pik-pik_qmr-main -->
  <!-- interface-description PIK-PIK QuickMacroRFID -->
  <!-- interface-copyright GPL-3.0-or-later -->
  <!-- interface-authors Valentin the Degu <degu@pik-pik.ee> -->
  <requires lib="gtk" version="4.6"/>
  <template class="PikPikQmrWindow" parent="GtkApplicationWindow">
    <property name="can-focus">False</property>
    <property name="default-height">480</property>
    <property name="default-width">640</property>
    <property name="show-menubar">True</property>
    <property name="title">PIK-PIK QuickMacroRFID 0.1</property>
    <child>
      <object class="GtkBox" id="box_main">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="label">
            <property name="can-focus">False</property>
            <property name="halign">start</property>
            <property name="label">~(__^&gt; test test test &lt;^__)~</property>
            <property name="margin-bottom">5</property>
            <property name="margin-end">5</property>
            <property name="margin-start">5</property>
            <property name="margin-top">5</property>
            <property name="valign">start</property>
          </object>
        </child>
      </object>
    </child>
  </template>
  <object class="GtkDialog" id="dialog_card_tool">
    <property name="title" translatable="yes">Card Tool</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkLabel">
                <property name="label">ID:</property>
                <property name="margin-bottom">10</property>
                <property name="margin-end">10</property>
                <property name="margin-start">10</property>
                <property name="margin-top">10</property>
              </object>
            </child>
            <child>
              <object class="GtkText">
                <property name="margin-bottom">10</property>
                <property name="margin-end">10</property>
                <property name="margin-start">10</property>
                <property name="margin-top">10</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="yes">Status: </property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label_status"/>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkButton" id="button_read_id">
                <property name="label" translatable="yes">Read ID</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_write_id">
                <property name="label" translatable="yes">Write ID</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_cancel">
                <property name="label" translatable="yes">Cancel</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
