            "sources" : [
                {
                    "type" : "git",
                    "url" : "file:///home/valka/Projects"
                }
            ],
            "config-opts" : [
                ""
            ]
        }
    ],
    "build-options" : {
        "cflags": "-O0 -g -Wno-declaration-after-statement -Wno-unused-function",
        "cxxflags": "-O0 -g -Wno-declaration-after-statement -Wno-unused-function",
        "env" : { "V": "1" }
